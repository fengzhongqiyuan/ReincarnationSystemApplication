<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.edu.zknu.reincarnationsystemapplication">

    <application
        android:allowBackup="true"
        android:icon="@drawable/desk"
        android:label="@string/app_name"
        android:roundIcon="@drawable/desk"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SecondStepActivity" />
        <activity android:name=".ThirdStepActivity" />
        <activity android:name=".RechargeActivity" />
        <activity android:name=".FourthStepActivity" />
        <activity android:name=".FifthStepActivity" />
        <activity android:name=".SixthStepActivity" />
        <activity android:name=".SeventhStepActivity" />
        <activity android:name=".EighthStepActivity" />
        <activity android:name=".NinthStepActivity" />
        <activity android:name=".TenthStepActivity" />
        <activity android:name=".FinalStepActivity" />
        <activity android:name=".ContextMenuActivity" />
        <activity android:name=".NoHaveSkipActivity"></activity>
    </application>

</manifest>